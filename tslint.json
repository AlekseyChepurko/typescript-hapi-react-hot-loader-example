{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-immutable",
    "tslint-eslint-rules"
  ],
  "rules": {
    "no-trailing-whitespace": false,
    "newline-before-return": true,
    "ordered-imports": [false],
    "arrow-parens": [true],
    "member-ordering": [true, {"order": "fields-first"}],
    "quotemark": [
      true,
      "avoid-escape",
      "jsx-double",
      "single"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case",
      "ban-keywords",
      "check-format"
    ],
    "max-line-length": [
      true,
      200
    ],
    "trailing-comma": [
        true,
        {"multiline": {"functions": "never"}}
    ],
    "jsx-no-multiline-js": false,
    "jsx-wrap-multiline": false
  },
  "rulesDirectory": []
}
